<?xml version="1.0" encoding="WINDOWS-1251"?>
        <!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="JDI-Light Angular Tests" parallel="tests" thread-count="3">
    <test name="Common elements tests">
        <packages>
            <package name="io.github.epam.angular.tests.elements.common.*"/>
        </packages>
    </test>
    <test name="Complex elements tests">
        <packages>
            <package name="io.github.epam.angular.tests.elements.complex.*"/>
        </packages>
    </test>
    <test name="Unit tests">
        <packages>
            <package name="io.github.epam.angular.tests.unit.*"/>
        </packages>
    </test>
   <listeners>
        <listener class-name="io.github.epam.testng.TestNGListener" />
    </listeners>
</suite>
